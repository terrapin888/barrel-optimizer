{
  "name": "barrel-optimizer",
  "version": "1.0.0",
  "description": "Zero-Overhead Barrel File Optimizer for better tree-shaking",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "barrel-optimizer": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "optimize": "node dist/cli/index.js optimize",
    "analyze": "node dist/cli/index.js analyze",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "barrel",
    "tree-shaking",
    "optimizer",
    "imports",
    "bundler",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@mui/material": "^7.3.5",
    "@swc/core": "^1.3.100",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "es-module-lexer": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  }
}
